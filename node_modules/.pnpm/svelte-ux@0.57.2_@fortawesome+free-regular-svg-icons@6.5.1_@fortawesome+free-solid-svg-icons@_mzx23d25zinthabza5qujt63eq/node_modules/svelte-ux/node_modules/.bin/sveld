#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jukim/Desktop/project/ComponentLibrary/node_modules/.pnpm/sveld@0.18.1_postcss@8.4.31/node_modules/sveld/node_modules:/Users/jukim/Desktop/project/ComponentLibrary/node_modules/.pnpm/sveld@0.18.1_postcss@8.4.31/node_modules:/Users/jukim/Desktop/project/ComponentLibrary/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jukim/Desktop/project/ComponentLibrary/node_modules/.pnpm/sveld@0.18.1_postcss@8.4.31/node_modules/sveld/node_modules:/Users/jukim/Desktop/project/ComponentLibrary/node_modules/.pnpm/sveld@0.18.1_postcss@8.4.31/node_modules:/Users/jukim/Desktop/project/ComponentLibrary/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sveld@0.18.1_postcss@8.4.31/node_modules/sveld/cli.js" "$@"
else
  exec node  "$basedir/../../../../../sveld@0.18.1_postcss@8.4.31/node_modules/sveld/cli.js" "$@"
fi
